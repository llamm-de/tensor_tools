module test_physics_tensors
    use funit
    use libtt_physics_tensors

    implicit none

contains

    @test
    subroutine assert_rightCauchyGreen()
        real(kind=8), dimension(3,3) :: defGrad
        real(kind=8), dimension(3,3) :: rightCauchyGreen

        defGrad = reshape((/1, 2, 3, 4, 5, 6, 7, 8, 9/),&
                          (/3,3/))
        rightCauchyGreen = reshape((/14, 32, 50, 32, 77, 122, 50, 122, 194/),&
                                   (/3,3/))

        @assertEqual(rightCauchyGreen, getRightCauchyGreen(defGrad))
    end subroutine assert_rightCauchyGreen

    @test
    subroutine assert_leftCauchyGreen()
        real(kind=8), dimension(3,3) :: defGrad
        real(kind=8), dimension(3,3) :: leftCauchyGreen

        defGrad = reshape((/1, 2, 3, 4, 5, 6, 7, 8, 9/),&
                          (/3,3/))

        leftCauchyGreen = reshape((/66, 78, 90, 78, 93, 108, 90, 108, 126/),&
                                  (/3,3/))

        @assertEqual(leftCauchyGreen, getLeftCauchyGreen(defGrad))
    end subroutine assert_leftCauchyGreen


end module test_physics_tensors